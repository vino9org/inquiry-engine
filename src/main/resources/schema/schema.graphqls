type Query {
    CasaAccount(accountId: String!): CasaAccount
    CasaAccountsByCustomer(customerId: String!): [CasaAccount]
}

type CasaTransaction {
    accountId: String!
    customerId: String!
    refId: String!
    amount: String!
    currency: String!
    valueDate: String!
    memo: String!
}

type CasaAccount {
    accountId: String!
    currency: String!
    balance: String!
    transactions: [CasaTransaction]
}