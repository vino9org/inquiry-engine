type Query {
    getCasaAccountDetail(accountId: String, trxStartDat: String, trxEndDate: String): [CasaAccount]
}

type CasaTransaction {
    refId: String!
    amount: String!
    currency: String!
    valueDate: String!
    memo: String!
}

type CasaAccount {
    accountId: String!
    currency: String!
    balance: String!
    transactions: [CasaTransaction]
}