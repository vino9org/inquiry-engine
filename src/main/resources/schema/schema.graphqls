type Query {
    getCasaAccountDetail(accountId: String!): CasaAccount
    getTransactionsForCasaAccount(accountId: String!): [CasaTransaction]
    getCasaAccountDetailWithTransactions(accountId: String!): CasaAccount
}

type CasaTransaction {
    accountId: String!
    refId: String!
    amount: String!
    currency: String!
    valueDate: String!
    memo: String!
}

type CasaAccount {
    accountId: String!
    currency: String!
    balance: String!
    transactions: [CasaTransaction]
}